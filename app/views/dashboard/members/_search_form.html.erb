<%= search_form_for @q, url: dashboard_members_url, remote: true, class: "form-inline" do |f| %>
  <div class="form-group mb-2">
    <%= f.label :name_cont, t(".search_member") %>
  </div>
  <div class="form-group mx-sm-3 mb-2">
    <%= f.search_field :name_or_email_cont, class: "form-control" %>
  </div>
    <%= f.submit t(".search"), class: "btn btn-primary mb-2"  %>
  </div>
<% end %>
