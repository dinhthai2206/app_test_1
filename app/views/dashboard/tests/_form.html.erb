<%= form_for [:dashboard, @test] do |f| %>
  <%= render "shared/error_messages", object: @test %>
  <div class="form-group">
    <%= f.label :categogy %>
    <%= f.select :category_id, options_from_collection_for_select(Category.all, :id, :title) %>
  </div>
  <h3><%= t ".questions" %></h3>
  <div id="questions">
    <%= f.fields_for :questions do |question| %>
      <%= render "question_fields", f: question %>
    <% end %>
  </div>
  <div>
    <%= link_to_add_association t(".add_question"), f, :questions, class: "link btn btn-primary" %>
  </div>
  <div class="text-center">
    <%= f.submit "Submit", class:"btn btn-success" %>
  </div>
<% end %>
