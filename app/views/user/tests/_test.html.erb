<tr>
  <td><%= t(".test", id: test.id) %></td>
  <td><%= test.questions.count %></td>
  <td><%= test.members.count %></td>
  <% if current_user_member.tests.include? test %>
    <td><%= link_to t(".done"), "#", class: "btn btn-secondary disabled" %></td>
  <% else %>
    <td><%= link_to t(".join"), join_user_test_path(test), class: "btn btn-success" %></td>
  <% end %>
</tr>
