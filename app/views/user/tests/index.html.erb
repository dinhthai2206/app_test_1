<h2><%= t ".all_tests" %></h2><hr/>

<div>
  <%= form_tag user_published_tests_path, method: :get, class: "form-inline" do %>
    <div class="form-group mb-2">
      <%= label :category_id, t(".select") %>
    </div>
    <div class="form-group mx-sm-3 mb-2">
      <%= select_tag :category_id, options_for_select(@category.map{|category| [category.title, category.id]}, selected: params["category_id"]),
      prompt: "--", class: "form-control" %>
    </div>
    <div><%= submit_tag t(".get"), class: "btn btn-primary mb-2" %></div>
  <% end %>
</div><hr/>

<div>
  <%= render "table_test"%>
</div>
